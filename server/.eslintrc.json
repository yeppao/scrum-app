{
  "env": {
    "node": true
  },
  "extends": [
    "plugin:import/typescript"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "plugins": ["@typescript-eslint", "import"],
  "rules": {
    "no-console": "warn"
  },
  "settings": {
    "import/resolver": {
      "typescript": {}
    }
  }
}
